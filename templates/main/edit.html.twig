{% extends "base.html.twig" %}

{% block body %}
<h1>Modification du formulaire</h1>
    <div class="container ">
    {{form_start(form)}}
    {{form_row(form.titre)}}
    {{form_row(form.description)}}
    {{form_row(form.couleur)}}
    {{form_row(form.taille)}}
    {{form_row(form.collection)}}
    {{form_row(form.photo)}}
    {{form_row(form.prix)}}
    {{form_row(form.stock)}}
    <button type="submit" class="btn btn-primary">Modifier</button>
    {{form_end(form)}}

    </div>

{% endblock %}